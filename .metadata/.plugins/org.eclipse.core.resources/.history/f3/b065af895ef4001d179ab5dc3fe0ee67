<%
String sel = request.getParameter("sel");
String vol = request.getParameter("vol");
int svol = Integer.parseInt(vol);
int num = Integer.parseInt(vol);

if(sel != null && sel.equals("gugudanType1")){
	response.sendRedirect("gugudan.jsp?num="+ num);
	
} else { 
// 	response.sendRedirect("gugudan2.jsp?col=" + svol);
	request.getRequestDispatcher("gugudan2.jsp?col=" + svol).forward(request, response);
}

%>

